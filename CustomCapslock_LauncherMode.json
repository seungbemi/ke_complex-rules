{
    "title": "CustomCapsLock : LauncherMode (CapsLock+Enter)",
    "rules": [
        {
            "description": "CustomCapsLock | LauncherMode : Enter ## Mandatory Field ##",
            "manipulators": [
                {
                    "conditions": [
                        {
                            "name": "capslock_mode",
                            "type": "variable_unless",
                            "value": 0
                        }
                    ],
                    "from": {
                        "key_code": "return_or_enter",
                        "modifiers": {
                            "optional": [
                                "caps_lock"
                            ]
                        }
                    },
                    "to": [
                        {
                            "set_variable": {
                                "name": "capslock_mode",
                                "value": 2
                            }
                        }
                    ],
                    "type": "basic"
                }
            ]
        },
        {
            "description": "CustomCapsLock | LauncherMode : Brave Browser (b)",
            "manipulators": [
                {
                    "conditions": [
                        {
                            "name": "capslock_mode",
                            "type": "variable_if",
                            "value": 2
                        }
                    ],
                    "from": {
                        "key_code": "b",
                        "modifiers": {
                            "optional": [
                                "caps_lock"
                            ]
                        }
                    },
                    "to": [
                        {
                            "shell_command": "open -a 'Brave Browser.app'"
                        }
                    ],
                    "type": "basic"
                }
            ]
